server {
    listen 80;
    server_name woman.ji;

    location /woman/doc/ {
        proxy_pass http://woman.ji:1939/;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_set_header Host $host;
    }

    location /woman/audit/ {
        proxy_pass http://woman.ji:1989/;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_set_header Host $host;
    }

}

